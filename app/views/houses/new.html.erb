<h1>Create a New G.O.T. House</h1>

<%= form_for @house do |f|%>

  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :motto %>
  <%=f.text_field :motto %>

  <%=f.label :region %>
  <%=f.text_field :region %>

  <%=f.label :crest_url %>
  <%=f.text_field :crest_url %>

  <%= f.submit "Create"%>
