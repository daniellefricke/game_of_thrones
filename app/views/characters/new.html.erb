<h1>Create a New G.O.T. Character</h1>

<%= form_for @character do |f|%>

  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :age %>
  <%=f.text_field :age %>

  <%=f.label :img_url %>
  <%=f.text_field :img_url %>

  <%=f.label :house %>
  <%=f.text_field :house %>

  <%= f.submit "Create"%>
