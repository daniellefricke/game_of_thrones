<h1>Edit this Character</h1>

<%= form_for [@house, @character] do |f|%>

  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :age %>
  <%=f.text_field :age %>

  <%=f.label :img_url %>
  <%=f.text_field :img_url %>

  <%= f.submit "Edit"%>
<%end%>

<%= form_for [@house, @character], method: :delete do |f| %>
  <%= f.submit "Delete this Character!", class: "delete" %>
<% end %>
